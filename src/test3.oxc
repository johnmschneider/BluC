#include <stdio.h>

class Example
{
    int memberVariable;
    
    this()
    {
        this->memberVariable = 22;
    }
    
    //destructor here
    ~this()
    {
        printf("Destructor called\n");
    }
    
    int getVariable()
    {
        return this->memberVariable;
    }
}

int main(void)
{
    Example ex;
    
    //late constructor call here
    ex();
    
    printf("variable == %i\n", ex.getVariable());
    return 0;
}